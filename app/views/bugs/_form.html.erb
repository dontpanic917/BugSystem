<%= form_for @bug do |f|%>
  <div class="col-lg-12 mb-6">
    <%= f.label "Solution's Description"%><br><br>
    <%= f.text_area :solution_description%><br><br>
  </div>
  <div class="col-lg-12 mb-6">
  <%= f.check_box :completed, {}, true, false%>
  <%= f.label "Solved"%><br><br>
</div>
<div>
  <%= f.submit "Submit",class: "btn btn-info"%>
</div>
<%end%>
