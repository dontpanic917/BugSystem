<div>
  <h1>Bugs</h1>
</div>

<div>
  <%=form_for @customer.bugs.build do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>
    <%= f.label :description%>
    <%= f.text_field :description%>
    <%= f.hidden_field :customer_id, value: @customer.id%>
    <%=f.submit "Create Bug"%>
  <%end%>
</div>
